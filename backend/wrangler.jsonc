{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "voter15-backend",
  "main": "src/index.ts",
  "compatibility_date": "2025-07-09",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "kv_namespaces": [
    {
      "binding": "VOTER_KV",
      "id": "${KV_ID}",
      "preview_id": "${KV_PREVIEW_ID}"
    }
  ],
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "voter-db",
      "database_id": "${D1_DATABASE_ID}"
    }
  ]
}
